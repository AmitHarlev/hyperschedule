{
  "babel": {
    "plugins": [
      "./babel-plugin.js"
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.6.1"
  },
  "devDependencies": {
    "express": "4.16.2",
    "npm-run-all": "4.1.2"
  },
  "engines": {
    "node": ">=8.8.1"
  },
  "license": "MIT",
  "name": "hyperschedule",
  "scripts": {
    "build": "rm -rf out && babel src -d out -D",
    "clean": "rm -rf out",
    "dev": "run-p server watch",
    "server": "./server.js",
    "watch": "rm -rf out && babel src -d out -D -w -q"
  },
  "version": "0.0.0"
}
